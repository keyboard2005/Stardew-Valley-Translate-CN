{
  "Format": "2.5.3",
  "ConfigSchema": {
    "EarthObeliskEnabled": {
      "AllowValues": "true, false",
      "Default": "true",
      "AllowBlank": false,
      "Description": "Enable or disable the Earth Obelisk replacement",
      "Section": "General"
    },
    "DesertObeliskEnabled": {
      "AllowValues": "true, false",
      "Default": "true",
      "AllowBlank": false,
      "Description": "Enable or disable the Desert Obelisk replacement",
      "Section": "General"
    },
    "WaterObeliskEnabled": {
      "AllowValues": "true, false",
      "Default": "true",
      "AllowBlank": false,
      "Description": "Enable or disable the Water Obelisk replacement",
      "Section": "General"
    },
    "IslandObeliskEnabled": {
      "AllowValues": "true, false",
      "Default": "true",
      "AllowBlank": false,
      "Description": "Enable or disable the Island Obelisk replacement",
      "Section": "General"
    },
    "FarmObeliskEnabled": {
      "AllowValues": "true, false",
      "Default": "true",
      "AllowBlank": false,
      "Description": "Enable or disable the Farm Obelisk replacement",
      "Section": "General"
    },
    "FlowersEnabled": {
      "AllowValues": "true, false",
      "Default": "true",
      "AllowBlank": false,
      "Description": "Enable or disable decorative flowers around obelisks",
      "Section": "Appearance"
    },
    "MossEnabled": {
      "AllowValues": "true, false",
      "Default": "true",
      "AllowBlank": false,
      "Description": "Enable or disable decorative moss around obelisks",
      "Section": "Appearance"
    },
    "MiniObeliskEnabled": {
      "AllowValues": "true, false",
      "Default": "true",
      "AllowBlank": false,
      "Description": "Enable or disable Mini-Obelisk replacements",
      "Section": "MiniObelisks"
    },
    "MiniObeliskStyle": {
      "AllowValues": "signpost, statue",
      "Default": "signpost",
      "AllowBlank": false,
      "Description": "Choose between signpost or statue style for Mini-Obelisks",
      "Section": "MiniObelisks"
    },
    "RemoteObeliskEnabled": {
      "AllowValues": "true, false",
      "Default": "true",
      "AllowBlank": false,
      "Description": "Enable or disable the Remote Obelisk replacement",
      "Section": "Remote"
    },
    "RemoteWarpLocation": {
      "AllowValues": "default, custom",
      "Default": "default",
      "AllowBlank": false,
      "Description": "Choose between default or custom warp location for Remote Obelisk",
      "Section": "Remote"
    },
    "CustomFarmX": {
      "Default": 48,
      "AllowBlank": false,
      "Description": "X coordinate for custom Remote Obelisk teleport location",
      "Section": "Remote"
    },
    "CustomFarmY": {
      "Default": 7,
      "AllowBlank": false,
      "Description": "Y coordinate for custom Remote Obelisk teleport location",
      "Section": "Remote"
    },
    "EastScarpEnabled": {
      "AllowValues": "true, false",
      "Default": "true",
      "AllowBlank": false,
      "Description": "Enable or disable East Scarp compatibility",
      "Section": "Compatibility"
    },
    "RidgesideVillageEnabled": {
      "AllowValues": "true, false",
      "Default": "true",
      "AllowBlank": false,
      "Description": "Enable or disable Ridgeside Village compatibility",
      "Section": "Compatibility"
    }
  },
  "DynamicTokens": [
    {
      "Name": "RemoteX",
      "Value": "{{CustomFarmX}}",
      "When": {
        "RemoteWarpLocation": "custom"
      }
    },
