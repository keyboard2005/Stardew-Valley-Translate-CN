      "LogName": "jennajuffuffles.obelisk.includes.miniobelisk",
      "Action": "Include",
      "FromFile": "content/miniobelisk/{{MiniObeliskStyle}}_MiniObelisk.json",
      "When": {
        "HasFile:{{FromFile}}": true,
        "MiniObeliskEnabled": "true",
        "HasValue:MiniObeliskEnabled": true,
        "HasValue:MiniObeliskStyle": true,
        "MiniObeliskStyle": "signpost, statue"
