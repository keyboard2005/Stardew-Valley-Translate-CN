  "Changes": [
    {
      "LogName": "jennajuffuffles.obelisk.includes.earth",
      "Action": "Include",
      "FromFile": "content/obelisk/Earth.json",
      "When": {
        "HasFile:{{FromFile}}": true,
        "EarthObeliskEnabled": "true"
      },
      "Update": "OnLocationChange"
    },
    {
      "LogName": "jennajuffuffles.obelisk.includes.desert",
      "Action": "Include",
